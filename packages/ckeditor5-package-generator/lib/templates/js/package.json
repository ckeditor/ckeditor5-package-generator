{
  "name": "<%= packageName %>",
  "version": "0.0.1",
  "description": "A plugin for CKEditor 5.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature",
    "ckeditor5-plugin",
    "ckeditor5-package-generator"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./browser/*": null,
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "files": [
    "dist",
    "ckeditor5-metadata.json"
  ],
  "dependencies": {},
  "peerDependencies": {
    "ckeditor5": ">=48.0.0 || ^0.0.0-nightly"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-inspector": "^<%= packageVersions.ckeditor5Inspector %>",
    "@ckeditor/ckeditor5-package-tools": "<%= packageVersions.packageTools %>",
    "@vitest/browser-webdriverio": "^4.0.6",
    "@vitest/coverage-istanbul": "^4.0.6",
    "ckeditor5": "latest",
    "eslint": "^9.27.0",
    "eslint-config-ckeditor5": "^<%= packageVersions.eslintConfigCkeditor5 %>",
    "eslint-plugin-ckeditor5-rules": "^<%= packageVersions.eslintPluginCkeditor5Rules %>",
    "globals": "^16.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "stylelint": "^16.0.0",
    "stylelint-config-ckeditor5": "^<%= packageVersions.stylelintConfigCkeditor5 %>",
    "typescript": "~5.5.4",
    "vite": "^7.3.0",
    "vite-plugin-svgo": "^2.0.0",
    "vitest": "^4.0.6",
    "webdriverio": "^9.19.2"
  },
  "overrides": {
    "sugarss": "^5.0.1"
  },
  "scripts": {
    "start": "vite",
    "build": "<%= packageManager %> run build:npm && <%= packageManager %> run build:browser",
    "build:npm": "vite build --mode npm",
    "build:browser": "vite build --mode browser",
    "test": "vitest",
    "test:debug": "vitest --inspect-brk --no-file-parallelism --browser.headless=false",
    "lint": "eslint",
    "stylelint": "stylelint --quiet --allow-empty-input 'theme/**/*.css'",
    "translations:synchronize": "ckeditor5-package-tools translations:synchronize",
    "translations:validate": "ckeditor5-package-tools translations:synchronize --validate-only",
    "prepublishOnly": "<%= packageManager %> run build"
  },
  "lint-staged": {
    "**/*": [
      "eslint"
    ],
    "**/*.css": [
      "stylelint --quiet --allow-empty-input"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
